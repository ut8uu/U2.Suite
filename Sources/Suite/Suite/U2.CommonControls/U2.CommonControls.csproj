<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>Library</OutputType>
		<TargetFramework>net7.0</TargetFramework>
		<Nullable>disable</Nullable>
		<ApplicationIcon />
		<StartupObject />
		<AssemblyVersion>1.1.0.20414</AssemblyVersion>
		<FileVersion>1.1.0.20414</FileVersion>
	</PropertyGroup>
	<ItemGroup>
		<None Remove=".gitignore" />
	</ItemGroup>
  <ItemGroup>
    <PackageReference Include="Avalonia" Version="11.0.0-rc2.1" />
    <PackageReference Include="Avalonia.Desktop" Version="11.0.0-rc2.1" />
    <!--Condition below is needed to remove Avalonia.Diagnostics package from build output in Release configuration.-->
    <PackageReference Condition="'$(Configuration)' == 'Debug'" Include="Avalonia.Diagnostics" Version="11.0.0-rc2.1" />
    <PackageReference Include="Avalonia.ReactiveUI" Version="11.0.0-rc2.1" />
    <PackageReference Include="MessageBox.Avalonia" Version="2.3.1-rc1-1" />
    <PackageReference Include="PropertyChanged.Fody" Version="4.1.0" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Views\OkCancelButtonsUserControl.axaml.cs">
      <DependentUpon>%(Filename)</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>
